    {
      "name": "support",
      "portMappings": [
        {
          "containerPort": 8082,
          "hostPort": 8082,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        { "name": "ENV_TYPE", "value": "STAGING" },
        { "name": "DD_SERVICE", "value": "isomer-support" },
        { "name": "DD_TAGS", "value": "team:isomer,service:isomer-support" }
      ],
      "mountPoints": [
