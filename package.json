{
  "name": "isomercms",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/bin/www.js",
    "dev:services": "docker compose up -d",
    "dev": "source .env && ts-node-dev --respawn ./bin/www.js",
    "test": "source .env.test && jest",
    "release": "npm version $npm_config_isomer_update && git push --tags",
    "lint": "npx eslint .",
    "format": "npx prettier --write .",
    "check-format": "npx prettier --check .",
    "prepare": "husky install",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.24.0",
    "auto-bind": "^4.0.0",
    "aws-sdk": "^2.946.0",
    "axios": "^0.21.3",
    "base-64": "^0.1.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "debug": "~2.6.9",
    "dompurify": "^2.3.1",
    "dotenv": "^10.0.0",
    "exponential-backoff": "^3.1.0",
    "express": "~4.16.1",
    "file-type": "^16.5.3",
    "helmet": "^4.6.0",
    "http-errors": "~1.8.0",
    "is-svg": "^4.3.1",
    "joi": "^17.4.0",
    "js-base64": "^2.6.4",
    "jsdom": "^16.7.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment-timezone": "^0.5.33",
    "morgan": "~1.10.0",
    "otplib": "^12.0.1",
    "pg": "^8.6.0",
    "pg-connection-string": "^2.5.0",
    "query-string": "^6.14.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.17.0",
    "sequelize-typescript": "^2.1.3",
    "serialize-error": "^7.0.1",
    "slugify": "^1.6.0",
    "supertest": "^6.1.3",
    "toml": "^3.0.0",
    "ts-node": "^10.7.0",
    "type-fest": "^2.12.0",
    "umzug": "^3.0.0",
    "uuid": "^3.3.3",
    "validator": "^13.6.0",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^3.1.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.56",
    "@swc/core": "^1.2.160",
    "@swc/helpers": "^0.3.8",
    "@tsconfig/recommended": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "auto-changelog": "^2.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.5.1",
    "jest-mock-axios": "^4.5.0",
    "lint-staged": "^11.1.2",
    "prettier": "2.2.1",
    "regenerator-runtime": "^0.13.9",
    "sqlite3": "^5.0.2",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "_moduleAliases": {
    "@root": ".",
    "@classes": "classes",
    "@errors": "errors",
    "@logger": "logger",
    "@middleware": "middleware",
    "@routes": "routes",
    "@utils": "utils",
    "@loaders": "loaders",
    "@database": "database",
    "@services": "services",
    "@validators": "validators",
    "@fixtures": "fixtures"
  },
  "lint-staged": {
    "**/*.(js|jsx|ts|tsx)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.17.5"
  }
}
