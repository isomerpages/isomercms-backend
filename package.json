{
  "name": "isomercms",
  "version": "0.12.0",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register build/server.js",
    "dev:services": "docker compose up -d",
    "dev": "source .env && ts-node-dev --respawn src/server.js",
    "test": "source .env.test && jest",
    "release": "npm version $npm_config_isomer_update && git push --tags",
    "lint": "npx eslint .",
    "lint-fix": "eslint --ignore-path .gitignore . --fix",
    "format": "npx prettier --check .",
    "format-fix": "npx prettier --write .",
    "prepare": "husky install",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "db:migrate": "source .env && npx sequelize-cli db:migrate",
    "db:migrate:undo": "source .env && npx sequelize-cli db:migrate:undo",
    "jump:staging": "source .ssh/.env.staging && ssh -L 5433:$DB_HOST:5432 $SSH_USER@$SSH_HOST -i .ssh/isomercms-staging-bastion.pem",
    "db:migrate:staging": "source .ssh/.env.staging && npx sequelize-cli db:migrate",
    "jump:prod": "source .ssh/.env.prod && ssh -L 5433:$DB_HOST:5432 $SSH_USER@$SSH_HOST -i .ssh/isomercms-production-bastion.pem",
    "db:migrate:prod": "source .ssh/.env.prod && npx sequelize-cli db:migrate"
  },
  "dependencies": {
    "@aws-sdk/client-amplify": "^3.112.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.179.0",
    "@aws-sdk/client-secrets-manager": "^3.24.0",
    "@octokit/rest": "^18.12.0",
    "@opengovsg/formsg-sdk": "^0.9.0",
    "auto-bind": "^4.0.0",
    "aws-sdk": "^2.946.0",
    "axios": "^0.21.3",
    "base-64": "^0.1.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dd-trace": "^2.9.1",
    "debug": "~2.6.9",
    "dompurify": "^2.3.1",
    "dotenv": "^16.0.1",
    "exponential-backoff": "^3.1.0",
    "express": "~4.16.1",
    "file-type": "^16.5.4",
    "helmet": "^4.6.0",
    "http-errors": "~1.8.0",
    "is-svg": "^4.3.1",
    "isomorphic-dompurify": "^0.24.0",
    "isomorphic-git": "^1.18.2",
    "joi": "^17.4.0",
    "js-base64": "^2.6.4",
    "jsdom": "^16.7.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment-timezone": "^0.5.35",
    "morgan": "~1.10.0",
    "neverthrow": "^4.3.1",
    "otplib": "^12.0.1",
    "pg": "^8.6.0",
    "pg-connection-string": "^2.5.0",
    "query-string": "^6.14.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.21.2",
    "sequelize-typescript": "^2.1.3",
    "serialize-error": "^7.0.1",
    "slugify": "^1.6.0",
    "supertest": "^6.1.3",
    "toml": "^3.0.0",
    "ts-node": "^10.7.0",
    "type-fest": "^2.12.0",
    "umzug": "^3.0.0",
    "uuid": "^3.3.3",
    "validator": "^13.6.0",
    "winston": "^3.3.3",
    "winston-cloudwatch": "^6.1.1",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@octokit/types": "^6.35.0",
    "@swc/cli": "^0.1.56",
    "@swc/core": "^1.2.160",
    "@swc/helpers": "^0.3.8",
    "@tsconfig/recommended": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.186",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "auto-changelog": "^2.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-mock-axios": "^4.5.0",
    "lint-staged": "^11.1.2",
    "prettier": "2.2.1",
    "regenerator-runtime": "^0.13.9",
    "sqlite3": "^5.0.8",
    "ts-jest": "^27.1.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "_moduleAliases": {
    "@root": "src",
    "@classes": "src/classes",
    "@errors": "src/errors",
    "@logger": "src/logger",
    "@middleware": "src/middleware",
    "@routes": "src/routes",
    "@utils": "src/utils",
    "@loaders": "src/loaders",
    "@database": "src/database",
    "@services": "src/services",
    "@validators": "src/validators",
    "@fixtures": "src/fixtures",
    "@constants": "src/constants"
  },
  "lint-staged": {
    "**/*.(js|jsx|ts|tsx)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=14.17.5"
  }
}
